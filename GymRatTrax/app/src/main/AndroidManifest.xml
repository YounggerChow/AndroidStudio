<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gymrattrax.gymrattrax" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HomeScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DailyWorkout"
            android:label="@string/title_activity_daily_workout" >
        </activity>
        <activity
            android:name=".CalorieNegation"
            android:label="@string/title_activity_calorie_negation" >
        </activity>
        <activity
            android:name=".FitnessProfile"
            android:label="@string/title_activity_fitness_profile" >
        </activity>
        <activity
            android:name=".Settings"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name=".ViewProgress"
            android:label="@string/title_activity_view_progress" >
        </activity>
    </application>

</manifest>
